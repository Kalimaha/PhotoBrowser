<html>
    <head>
        <title>{{title}}</title>
        <script src="https://code.jquery.com/jquery-2.2.2.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
    </head>
    <body>
        <div class="container">
            <header>
                <h1>
                    {{title}}
                </h1>
                <nav>
                    <a href="{{maker_id}}.html">
                        Back to {{maker_name}}
                    </a>
                    <span class="pull-right">
                        <a href="index.html">
                            Home Page
                        </a>
                    </span>
                </nav>
            </header>
            <hr>
            <div class="row">
                {{#each urls}}
                    <div class="col-xs-3">
                        <img class="img-responsive img-thumbnail" src="{{this}}">
                    </div>
                {{/each}}
            </div>
        </div>
    </body>
</html>